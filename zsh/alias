alias cp='cp -i'

alias gs='screen -r S1'

alias gs1='screen -r S1'

alias gs2='screen -r S2'

alias grs='git remote show'

alias grso='git remote show origin'

alias gl='git pull origin master'

alias gp='git push origin master'

alias gitb='git add .;git ci -m "a";git pull origin master;git push origin master;'

alias go='git add .;git ci -m "a";git pull origin master;git push origin master;'

alias gh='git add .;git ci -m "a";git pull github master;git push github master;'

alias py='/usr/bin/python3'

alias ab='for l in {a..z} ; do echo $l;done;';
